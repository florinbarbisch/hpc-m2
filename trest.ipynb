   "source": [
    "from multiprocessing import Pool\n",
    "\n",
    "def f(arg):\n",
    "    host, x = arg\n",
    "    print(host)\n",
    "    print(x)\n",
    "\n",
    "hosts = ['1.1.1.1', '2.2.2.2']\n",
    "args = ((host, \"test\") for host in hosts)\n",
    "pool = Pool(processes=5)\n",
    "pool.map_async(f, args)\n",
    "pool.close()\n",
    "pool.join()\n"
   ]
  }
